naRegion pos1 pos2 |	colRegiones := mundoVisual getMapa getRegiones.	pos1 := 75.	pos2 := 130.	colRegiones		do: [ :cadaRegion | 			unaRegion := RegionVisual crear: cadaRegion.			unaRegion position: pos1 @ pos2.			unaRegion				color:					(Color						r: 0						g: 200						b: 0						range: 255).			cajaRegiones addMorph: unaRegion.			pos1 := pos1 + 110.			pos1 >= 525				ifTrue: [ pos1 := 75.					pos2 := pos2 + 120 ] ]! !!MundoVisual methodsFor: 'Aplicaciones' stamp: 'Stefano 8/27/2018 18:18'!openInWorld	self openInWindow.! !!MundoVisual methodsFor: 'Aplicaciones' stamp: 'StefanoMatias 9/10/2018 19:10'!dibujarConquistadas	| unJugador unaRegion |	cajaRegiones		submorphsDo: [ :cadaRegionVisual | 			unaRegion := cadaRegionVisual getRegion.			unJugador := miJugador getJugador.			(unJugador tengoLaRegion: unaRegion)				ifTrue: [ cadaRegionVisual						color:							(Color								r: 200								g: 0								b: 0								range: 255) ]				ifFalse: [ cadaRegionVisual						color:							(Color								r: 0								g: 200								b: 0								range: 255) ] ]! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/10/2018 18:59'!pasarJugador	| jugadores |	jugadores := mundoVisual getJugadores.	posJugador := posJugador + 1.	miJugador setJugador: (jugadores at: posJugador % jugadores size + 1).	miJugador label: miJugador getJugador getNombre.	miJugador bounds: (miJugador position extent: 100 @ 30).	posJugador % jugadores size = 0		ifTrue: [  mundoVisual finalizarRonda ].	self dibujarContadorUnidades.	self dibujarContadorRegiones.	self dibujarContadorDinero! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/10/2018 20:18'!accionFinalizarTurno	| contJugadores ganador |	contJugadores := mundoVisual getJugadores size.	botonFinalizarTurno		mouseAction: [ contJugadores > 1				ifTrue: [ self pasarJugador.					self dibujarConquistadas ]				ifFalse: [ ganador := mundoVisual getJugadores at: 1.					UIManager inform: 'EL GANADOR ES ' , ganador getNombre asString ] ]! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/20/2018 15:37'!botonCrearUnidad	| origen |	botonCrearUnidad := BotonAccion new.	origen := self innerBounds origin.	"margen superior izquierdo"	botonCrearUnidad label: 'Crear unidad'.	" SIEMPRE VA PRIMERO"	botonCrearUnidad position: origen + (650 @ 130).	botonCrearUnidad bounds: (botonCrearUnidad position extent: 100 @ 50).	self addMorph: botonCrearUnidad! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/10/2018 19:51'!accionAtacar	botonAtacar		mouseAction: [ miJugador getJugador				atacar: (UIManager default request: 'Ingrese region origen')				defensa: (UIManager default request: 'Ingrese region destino')				contrincantes: mundoVisual getJugadores.			self dibujarContadorUnidades.			self dibujarContadorRegiones.			self dibujarContadorDinero ]! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/20/2018 15:39'!accionCrearUnidad	| tipoUnidad nombre |	botonCrearUnidad		mouseAction: [ tipoUnidad := UIManager default				chooseFrom: #('Soldado normal' 'Soldado plus' 'Tanque' 'Camion' 'Bombardero')				title: 'Elija un tipo de unidad'.			nombre := UIManager default request: 'Ingrese el nombre de la region'.			tipoUnidad  = 1				ifTrue: [ miJugador getJugador crearSoldado: nombre ].				tipoUnidad  = 2				ifTrue: [ miJugador getJugador crearSoldadoPlus: nombre ].				tipoUnidad  = 3				ifTrue: [ miJugador getJugador crearTanque: nombre ].				tipoUnidad  = 4				ifTrue: [ miJugador getJugador crearCamion: nombre ].				tipoUnidad  = 5				ifTrue: [ miJugador getJugador crearBombardero: nombre ].			self dibujarContadorUnidades.			self dibujarContadorRegiones.			self dibujarContadorDinero ]! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/10/2018 11:22'!accionSubirPasajero	| tipoTransporte nombre |	botonSubirPasajero 		mouseAction: [ tipoTransporte := UIManager default				chooseFrom: #('Tanque' 'Camion' 'Bombardero')				title: 'Elija un tipo de transporte'.			nombre := UIManager default request: 'Ingrese el nombre de la region'.			tipoTransporte = 1				ifTrue: [ miJugador getJugador subirAtanque: nombre ]				ifFalse: [ tipoTransporte = 2						ifTrue: [ miJugador getJugador subirAcamion: nombre ]						ifFalse: [ miJugador getJugador subirAbombardero: nombre ] ].			self dibujarContadorUnidades.			self dibujarContadorRegiones.			self dibujarContadorDinero ]! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/10/2018 11:12'!accionCrearEdificio	| tipoEdificio nombre |	botonCrearEdificio 		mouseAction: [ tipoEdificio := UIManager default				chooseFrom: #('Escuela militar' 'Fabrica' 'Fabrica militar' 'Aeropuerto')				title: 'Elija un tipo de edificio'.			nombre := UIManager default request: 'Ingrese el nombre de la region'.			tipoEdificio = 1				ifTrue: [ miJugador getJugador crearEscuelaMilitar: nombre ]				ifFalse: [ tipoEdificio = 2						ifTrue: [ miJugador getJugador crearFabrica: nombre ]						ifFalse: [ tipoEdificio = 3								ifTrue: [ miJugador getJugador crearFabricaMilitar: nombre ]								ifFalse: [ miJugador getJugador crearAeropuerto: nombre ] ] ].			self dibujarContadorUnidades.			self dibujarContadorRegiones.			self dibujarContadorDinero ]! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/4/2018 00:13'!botonCrearEdificio	| origen |	botonCrearEdificio := BotonAccion new.	origen := self innerBounds origin.	"margen superior izquierdo"	botonCrearEdificio  label: 'Crear edificio'.	" SIEMPRE VA PRIMERO"	botonCrearEdificio  position: origen + (650 @ 190).	botonCrearEdificio  bounds: (botonCrearEdificio position extent: 100 @ 50).	self addMorph: botonCrearEdificio ! !!MundoVisual methodsFor: 'Botones' stamp: 'StefanoMatias 9/4/2018 00:12'!botonAtacar	| origen |	botonAtacar := BotonAccion new.	origen := self innerBounds origin.	"margen superior izquierdo"	botonAtacar label: 'Atacar'.	" SIEMPRE 